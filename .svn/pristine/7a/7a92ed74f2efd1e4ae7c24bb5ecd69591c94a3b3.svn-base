div.c_list {
    background: none repeat scroll 0 0 #fff;
    border: 1px solid #444;
    height: auto;
    margin-top: 1px;
    max-height: 200px;
    overflow-x: hidden;
    overflow-y: scroll;
    padding: 5px 0;
    position: absolute;
    width: 150px;
}
div.c_list ul {
    margin: 0;
    padding: 0;
}
div.c_list ul li:hover, div.c_list ul li.li_hover {
    background: none repeat scroll 0 0 #79a7d8;
}
div.c_list ul li {
    cursor: pointer;
    height: 16px;
    line-height: 16px;
    list-style: none outside none;
    margin: 0;
    text-indent: 5px;
}
table.iostoreTable{border-bottom:1px solid #333;border-right:1px solid #333;}
table.iostoreTable td,table.iostoreTable th{padding:3px;border-top:1px solid #333;border-left:1px solid #333;}
table.iostoreTable th{background:#44EEee;}